{
  "name": "flow",
  "version": "1.0.0",
  "main": "build/app.js",
  "scripts": {
    "build": "pnpm run lint:check && pnpm run madge:check-build && tsc -b",
    "start": "node -r dotenv/config build/app.js",
    "dev": "cross-env DEBUG=flow* ts-node-dev --respawn --transpile-only src/app.ts",
    "watch": "tsc -w",
    "madge:check-build": "madge --circular --extensions js ./build",
    "lint:check": "eslint src --ext .ts,.tsx",
    "madge": "madge --circular --extensions ts,js src/",
    "check-deps": "depcheck",
    "postinstall": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@clerk/express": "^1.3.50",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/prettier": "^3.0.0",
    "@types/superagent": "^8.1.9",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.7",
    "dotenv": "^16.5.0",
    "express": "^4.21.0",
    "getmac": "^6.6.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.9.0",
    "nanoid": "3.3.4",
    "openai": "^4.97.0",
    "prettier": "^3.5.2",
    "superagent": "^10.1.0"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.8.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.11",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "madge": "^6.0.0",
    "nodemon": "^3.1.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.31.1"
  }
}
